{
    "environment": "development",
    "server": {
        "port": 3000,
        "host": "localhost"
    },
    "logging": {
        "level": "debug",
        "format": "json"
    },
    "repositories": {
        "excludedRepos": [
            "FRONT-END-PUBLIC",
            "MBS_ANGLO", 
            "MTS_ANGLO",
            "SCADMIN",
            "SERVICECATALOG", 
            "SOLUTIONCOMPONENTS"
        ]
    },
    "services": {
        "svn": {
            "protocol": "https://",
            "baseUrl": "${SVN_HOST}",
            "username": "${SVN_USERNAME}",
            "password": "${SVN_PASSWORD}"
        },
        "jenkins": {
            "baseUrl": "http://192.168.10.159",
            "username": "${JENKINS_USERNAME}",
            "apiToken": "${JENKINS_API_TOKEN}"
        }
    }
}
