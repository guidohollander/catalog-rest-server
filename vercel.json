{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "WWW-Authenticate",
          "value": "Basic realm=\"Secure Area\""
        }
      ]
    }
  ],
  "env": {
    "BASIC_AUTH_USERNAME": "service_catalog_api",
    "BASIC_AUTH_PASSWORD": "service_catalog_api_remote",
    "NODE_ENV": "production"
  }
}
